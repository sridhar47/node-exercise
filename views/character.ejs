<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title>node-exercise</title>
	<link rel="stylesheet" href="">
</head>
<body>
	<% if(typeof(people) != "undefined") {%>
		<table>
			<thead>
				<tr>
					<th>Name</th>
					<th>Height</th>
					<th>Mass</th>
				</tr>
			</thead>
			<tbody>
				<% for(var i=0;i<people.length;i++) {%>
					<tr>
						<td>
							<h3><%= people[i].name %></h3>
						</td>
						<td>
							<label><%= people[i].height %></label>
						</td>
						<td>
							<span><%= people[i].mass %></span>
						</td>
					</tr>
				<% } %>
			</tbody>
		</table>
	<% } %>
	<% if(typeof(name) != "undefined") {%>
		<h3><%= name %></h3>
	<% } %>
</body>
</html>